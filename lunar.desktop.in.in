[Desktop Entry]
_Name=Lunar File Manager
_Comment=Browse the filesystem with the file manager
_GenericName=File Manager
_Keywords=file manager;explorer;finder;browser;folders;directory;directories;partitions;drives;network;devices;rename;move;copy;delete;permissions;home;trash;
Exec=lunar %F
Icon=com.expidus.lunar
Terminal=false
StartupNotify=true
Type=Application
Categories=System;Core;GTK;FileTools;FileManager;
MimeType=inode/directory;
Actions=open-home;open-computer;open-trash;

[Desktop Action open-home]
_Name=Home
Exec=lunar %U

[Desktop Action open-computer]
_Name=Computer
Exec=lunar computer:///

[Desktop Action open-trash]
_Name=Trash
Exec=lunar trash:///
